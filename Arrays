##Import numpy
import numpy as np

## 1dimension
vector_1 = np.array([1,2,3])
print("The vector is:")
print(vector_1)
print()

## two dimensions
vector_2 = np.array([(1,2,3),(4,5,6)])
print("The vector is:")
print(vector_2)
